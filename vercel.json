{
  "builds": [
    {
      "src": "billing/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "billing/wsgi.py"
    }
  ],
  "functions": {
    "api/**": {
      "memory": 1024
    }
  }
}
